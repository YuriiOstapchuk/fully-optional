{
  "name": "fully-optional",
  "version": "0.0.3",
  "main": "dist/index.js",
  "sideEffects": false,
  "repository": "git@github.com:YuriiOstapchuk/fully-optional.git",
  "author": "Yurii Ostapchuk <y.ostapchuk1@gmail.com>",
  "license": "MIT",
  "keywords": [
    "maybe",
    "option",
    "null",
    "null-safe",
    "utils",
    "fp",
    "functional",
    "typescript"
  ],
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "rm -rf dist",
    "compile": "tsc -p .",
    "prebuild": "npm run clean",
    "build": "npm run compile",
    "check-compilation": "npm run compile -- --noEmit",
    "precommit": "npm run check-compilation && lint-staged",
    "prepublish": "npm run build"
  },
  "lint-staged": {
    "*.ts": [
      "tslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "devDependencies": {
    "@types/jest": "^24.0.11",
    "husky": "^1.3.1",
    "jest": "^24.7.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "ts-jest": "^24.0.2",
    "tslint": "^5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.4.2"
  }
}
